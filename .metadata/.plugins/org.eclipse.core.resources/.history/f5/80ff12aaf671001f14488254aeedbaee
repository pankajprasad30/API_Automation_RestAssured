public class Employee_POJO {

	public static void main(String[] args) 
	{
		Child_POJO chd=new Child_POJO();
		chd.setStreet("631-B C main");
		chd.setCity("Bangalore");
		chd.setPincode(786184);
		chd.setState("Karnataka");
		
